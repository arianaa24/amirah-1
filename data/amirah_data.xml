<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_amirah_website_productos" model="ir.cron">
        <field name="name">Publicar y despublicar producto en website</field>
        <field name="interval_number">6</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="website.model_website"/>
        <field name="code">model._actualizar_producto()</field>
        <field name="state">code</field>
    </record>
</odoo>
